{
  "name": "crypto-counter",
  "private": true,
  "description": "Crypto currency tickers",
  "version": "0.1",
  "keywords": [
    "gatsby",
    "crypto"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "format": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "dependencies": {
    "@wesbos/dump": "0.1.1",
    "axios": "0.21.2",
    "babel-plugin-styled-components": "1.13.2",
    "date-fns": "2.23.0",
    "gatsby": "2.32.13",
    "gatsby-node-helpers": "0.3.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-offline": "3.10.2",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-styled-components": "3.10.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-seo-component": "2.0.2",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "husky": "7.0.1",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uvacoder/crypto-data-ticker.git"
  },
  "bugs": {
    "url": "https://github.com/uvacoder/crypto-data-ticker/issues"
  },
  "main": "index.js",
  "homepage": "https://crypto-data-ticker.vercel.app"
}
